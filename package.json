{
  "name": "@iwmywn/generate-index",
  "author": "Hoang Anh Tuan <itsmethu2408@gmail.com>",
  "version": "1.1.2",
  "description": "CLI tool to auto-generate index.ts/js files for importing and exporting modules in a folder structure.",
  "homepage": "https://github.com/iwmywn/iwmywn-generate-index#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iwmywn/iwmywn-generate-index.git"
  },
  "bugs": "https://github.com/iwmywn/iwmywn-generate-index/issues",
  "exports": {
    ".": "./dist/index.mjs"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "bin": {
    "iwmywn-generate-index": "./dist/index.mjs"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub"
  },
  "keywords": [
    "cli",
    "index",
    "generator",
    "typescript",
    "export",
    "import",
    "auto"
  ],
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@types/node": "^22.14.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0"
  }
}
